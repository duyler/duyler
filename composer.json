{
    "name": "duyler/duyler",
    "type": "project",
    "description": "",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikhail Ilinsky",
            "email": "mail@milinsky.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.3",
        "duyler/builder": "dev-main",
        "duyler/http": "dev-main",
        "duyler/web": "dev-main",
        "duyler/aspect": "dev-main",
        "duyler/io": "dev-main",
        "duyler/orm": "dev-main",
        "duyler/console": "dev-main",
        "spiral/roadrunner-http": "^3.1",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "DataFixtures\\": "fixtures/",
            "Migrations\\": "migrations/"
        }
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    }
}
